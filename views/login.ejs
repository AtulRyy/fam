        <link rel="stylesheet" href="/static/css/style.scss">
        <link rel="stylesheet" href="/static/css/styleMobile.css">
        <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="mobile-web-app-capable" content="yes" />
